<Application
    x:Class="Yttrium_browser.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Yttrium_browser"
    xmlns:primitives="using:Microsoft.UI.Xaml.Controls.Primitives"
    xmlns:winui="using:Microsoft.UI.Xaml.Controls">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Cube.UI/Resources/ResourcesHub.xaml" />
                <ResourceDictionary Source="ms-appx:///ResourceDictionaries/TabViewItem.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Thickness x:Key="TabViewItemAddButtonContainerPadding">3,0,0,8</Thickness>
            <x:Double x:Key="TabViewItemAddButtonFontSize">14</x:Double>
        </ResourceDictionary>
    </Application.Resources>

</Application>
